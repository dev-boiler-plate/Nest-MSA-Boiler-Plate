{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/config": [
        "libs/config/src"
      ],
      "@app/config/*": [
        "libs/config/src/*"
      ],
      "@app/database": [
        "libs/database/src"
      ],
      "@app/database/*": [
        "libs/database/src/*"
      ],
      "@app/middleware": [
        "libs/middleware/src"
      ],
      "@app/middleware/*": [
        "libs/middleware/src/*"
      ],
      "@app/interceptor": [
        "libs/interceptor/src"
      ],
      "@app/interceptor/*": [
        "libs/interceptor/src/*"
      ],
      "@app/util": [
        "libs/util/src"
      ],
      "@app/util/*": [
        "libs/util/src/*"
      ],
      "@app/event": [
        "libs/event/src"
      ],
      "@app/event/*": [
        "libs/event/src/*"
      ],
      "@app/local-storage": [
        "libs/local-storage/src"
      ],
      "@app/local-storage/*": [
        "libs/local-storage/src/*"
      ],
      "@app/redis": [
        "libs/redis/src"
      ],
      "@app/redis/*": [
        "libs/redis/src/*"
      ],
      "@app/strategy": [
        "libs/strategy/src"
      ],
      "@app/strategy/*": [
        "libs/strategy/src/*"
      ],
      "@app/guard": [
        "libs/guard/src"
      ],
      "@app/guard/*": [
        "libs/guard/src/*"
      ],
      "@app/decorator": [
        "libs/decorator/src"
      ],
      "@app/decorator/*": [
        "libs/decorator/src/*"
      ]
    }
  }
}